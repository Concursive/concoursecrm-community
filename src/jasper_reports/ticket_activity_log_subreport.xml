	<queryString><![CDATA[SELECT t.ticketid,
sc.contract_number AS contractnumber,
sc.current_start_date AS startdate,
sc.current_end_date AS enddate,
sc.description AS description,
cat.description AS category,
type.description AS type,
ct.namelast AS lastname, ct.namefirst AS firstname,
response.description AS responsetime,
phone.description AS telephoneservice,
onsite.description AS onsiteservice,
email.description AS emailservice,
sc.total_hours_remaining AS hoursremaining
FROM ticket t
LEFT JOIN service_contract sc ON (t.link_contract_id = sc.contract_id )
LEFT JOIN lookup_sc_category cat ON (sc.category = cat.code)
LEFT JOIN lookup_sc_type type ON (sc.type = type.code)
LEFT JOIN contact ct ON (sc.contact_id = ct.contact_id)
LEFT JOIN lookup_response_model response ON (sc.response_time = response.code)
LEFT JOIN lookup_phone_model phone ON (sc.telephone_service_model = phone.code)
LEFT JOIN lookup_onsite_model onsite ON (sc.onsite_service_model = onsite.code)
LEFT JOIN lookup_email_model email ON (sc.email_service_model = email.code)
WHERE ticketid = $P{ticketid}]]></queryString>
