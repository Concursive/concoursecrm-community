	<queryString><![CDATA[SELECT o.org_id, o.name,
ld.description as department_name,
resvby.contact_id, resvby.namefirst, resvby.namelast,
t.resolvable
FROM ticket t
LEFT JOIN organization o ON (t.org_id = o.org_id)
LEFT JOIN contact resvby ON (t.resolvedby = resvby.user_id)
LEFT JOIN lookup_department ld ON (t.resolvedby_department_code = ld.code)
WHERE t.ticketid > 0
AND o.enabled = $P{TRUE}
AND t.resolvedby IS NOT NULL
AND t.trashed_date IS NULL
AND t.ticketid NOT IN (SELECT ticket_id FROM ticketlink_project)
AND t.entered >= $P{date_start}
AND t.entered <= $P{date_end}
$P!{orgid_where}
$P!{siteid_where}
ORDER BY o.name, ld.description, resvby.namelast, resvby.namefirst]]></queryString>
