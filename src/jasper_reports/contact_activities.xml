	<queryString><![CDATA[
    SELECT cn2.namelast as user_last_name, 
    cn2.namefirst as user_first_name,
    log.owner, log.contact_id, 
    log.alertdate as due_date,  
    pr.description as priority, 	
    tp.description as "type", 
    cn.namelast as con_last_name, 
    cn.namefirst as con_first_name, 
    log.subject as description
    FROM call_log log
    LEFT JOIN lookup_call_priority pr ON (log.priority_id = pr.code)
    LEFT JOIN lookup_call_types tp ON (log.call_type_id = tp.code)
    LEFT JOIN contact cn ON (log.contact_id = cn.contact_id)
    LEFT JOIN contact cn2 ON (log.owner = cn2.user_id)
    WHERE log.alertdate IS NOT NULL
    AND log.status_id = $P{COMPLETE_FOLLOWUP_PENDING}
    AND log.alertdate >= $P{date_start}
    AND log.alertdate <= $P{date_end}
    AND log.owner IN ($P!{userid_range})
    ORDER BY cn2.namelast, cn2.namefirst, log.alertdate, pr."level", tp."level"
  ]]></queryString>
