<CFSPermissions>
  <!-- Current max category id: 14, 11 and 12 are missing? -->
  <!-- Every individual permission has a unique name -->
  <!-- Permission Categories can be arranged in any order, but they must have a unique sequntial id that
       matches the id used in the CFS code -->
  <!-- Permissions within a Category are in the order to appear on-screen, and can be changed without
       concerns about the code-->
  <!-- Permission attributes include: View, Add, Edit, Delete (vaed) and can be in any order -->
  <permissions>
    <category id="13" name="System" folders="false" lookups="false">
      <permission name="globalitems-search" attributes="v---" description="Access to Global Search"/>
