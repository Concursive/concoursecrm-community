<data-import-config>
  <description>
    Imports ticket categoriess into Centric from an Excel file.
  </description>
  <reader class="com.darkhorseventures.apps.dataimport.excelreader.ExcelTicketCategoryReader">
    <excelFile>Tickets Classification for Demo.xls</excelFile>
  </reader>
  <writer class="com.darkhorseventures.apps.dataimport.cfshttpxmlwriter.CFSHttpXMLWriter">
    <ignoreClientId>true</ignoreClientId>
