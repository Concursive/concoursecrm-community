<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC
    "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<!--

The labels used as replaceable parameters below are used by
the build.xml script that the "ant" application uses to 
install the ASPCFS application.

Replaceable parameter  typical value / description
=====================  =============================================
             SiteCode  ds21 
                       (the name that Login will search
                       the gatekeeper database with)

               GKHOST  jdbc:postgresql://127.0.0.1:5432/cfs2gk
                       (Database URL: driver, host address, 
                       port, and database name of the
                       gatekeeper database that this installation
                       will use)

               GKUSER  gatekeeper
                       (gatekeeper database user name)

             GKUSERPW  (blank, empty, nada, ...)
                       (gatekeeper database user password)

-->
<web-app>
  <listener>
    <listener-class>
     org.aspcfs.controller.ContextListener
    </listener-class>
  </listener>
  
  <listener>
    <listener-class>
     org.aspcfs.controller.ContextSessionListener
    </listener-class>
  </listener>

  <servlet>
    <servlet-name>Controller</servlet-name>
    <display-name>Controller</display-name>
    <servlet-class>com.darkhorseventures.framework.servlets.ControllerServlet</servlet-class>
    <init-param>
        <param-name>Debug</param-name>
        <param-value>@DEBUGLEVEL@</param-value>
    </init-param>
    <init-param>
        <param-name>ActionConfig</param-name>
        <param-value>cfs-config.xml</param-value>
    </init-param>
    <init-param>
        <param-name>ModuleConfig</param-name>
        <param-value>cfs-modules.xml</param-value>
    </init-param>
    <init-param>
        <param-name>ContainerMenuConfig</param-name>
        <param-value>cfs-container_menus.xml</param-value>
    </init-param>
    <init-param>
        <param-name>DynamicFormConfig</param-name>
        <param-value>cfs-dynamic_forms.xml</param-value>
    </init-param>
    <init-param>
        <param-name>PopulateAttributeName</param-name>
        <param-value>auto-populate</param-value>
    </init-param>
    <init-param>
        <param-name>UseXSLCache</param-name>
        <param-value>false</param-value>
    </init-param>
    <init-param>
        <param-name>PopulateClassName</param-name>
        <param-value>com.darkhorseventures.framework.beans.BeanUtils</param-value>
    </init-param>
    <init-param>
        <param-name>InitHook</param-name>
        <param-value>org.aspcfs.controller.InitHook</param-value>
    </init-param>
    <init-param>
        <param-name>Hook</param-name>
        <param-value>org.aspcfs.controller.SecurityHook</param-value>
    </init-param>
    <init-param>
        <param-name>GlobalItemsHook</param-name>
        <param-value>org.aspcfs.controller.GlobalItemsHook</param-value>
    </init-param>
    <init-param>
        <param-name>MainMenuHook</param-name>
        <param-value>org.aspcfs.controller.MainMenuHook</param-value>
    </init-param>
    <init-param>
        <param-name>ForceSSL</param-name>
        <param-value>@FORCESSL@</param-value>
    </init-param>
    <init-param>
        <param-name>SiteCode</param-name>
        <param-value>@SiteCode@</param-value>
    </init-param>
    <init-param>
        <param-name>ClientSSLKeystore</param-name>
        <param-value>@KEYSTORE@</param-value>
    </init-param>
    <init-param>
        <param-name>ClientSSLKeystorePassword</param-name>
        <param-value>aspcfs</param-value>
    </init-param>
    <init-param>
        <param-name>GKDRIVER</param-name>
        <param-value>@GKDRIVER@</param-value>
    </init-param>
    <init-param>
        <param-name>GKHOST</param-name>
        <param-value>@GKHOST@</param-value>
    </init-param>
    <init-param>
        <param-name>GKUSER</param-name>
        <param-value>@GKUSER@</param-value>
    </init-param>
    <init-param>
        <param-name>GKUSERPW</param-name>
        <param-value>@GKUSERPW@</param-value>
    </init-param>
    <init-param>
        <param-name>MailServer</param-name>
        <param-value>@MAILSERVER@</param-value>
    </init-param>
    <init-param>
        <param-name>GlobalPWInfo</param-name>
        <param-value>118c0f0bb67a6058a911342a059bae03</param-value>
    </init-param>
    <init-param>
        <param-name>PreloadXSL</param-name>
        <param-value>false</param-value>
    </init-param>
    <init-param>
        <param-name>CacheModules</param-name>
        <param-value>true</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <session-config>
        <session-timeout>90</session-timeout>
  </session-config>
  <!--welcome-file-list>
	  <welcome-file>index.jsp</welcome-file>
  </welcome-file-list-->
</web-app>
