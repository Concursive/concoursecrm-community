	<queryString><![CDATA[SELECT t.ticketid, t.entered,
t.problem,  
ts.description,
co.namefirst,
co.namelast,
o.org_id, o.name
FROM ticket t
LEFT JOIN organization o ON (t.org_id = o.org_id)
LEFT JOIN ticket_severity ts ON (t.scode = ts.code) 
LEFT JOIN contact co ON (t.assigned_to = co.user_id)
WHERE t.ticketid > 0
AND o.enabled = $P{TRUE}
AND t.ticketid NOT IN (SELECT ticket_id FROM ticketlink_project) 
AND t.closed IS NULL
AND t.entered >= $P{date_start}
AND t.entered <= $P{date_end}
AND t.trashed_date IS NULL
$P!{orgid_where}
$P!{siteid_where}
ORDER BY t.entered, o.name]]></queryString>
