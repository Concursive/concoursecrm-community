	<queryString><![CDATA[SELECT 
    c.namefirst, c.namelast, c.contact_id, c.title, c.company,
    c.status_id, c.potential, ca.city, ca.postalcode,
    lct.description as contact_type,
    lind.description as business_segment,
    o.name 
  FROM contact c
  LEFT JOIN contact_type_levels ctl ON (c.contact_id = ctl.contact_id)
  LEFT JOIN lookup_contact_types lct ON (ctl.type_id = lct.code)
  LEFT JOIN lookup_industry lind ON (c.industry_temp_code = lind.code)
  LEFT JOIN contact_address ca ON (c.contact_id = ca.contact_id)
  LEFT JOIN organization o ON (c.org_id = o.org_id)
  WHERE c.owner IN ($P{userid_range})
  $P!{siteid_where}
  AND c.entered >= $P{date_range_start}
  AND c.entered <= $P{date_range_end}
  AND (c.status_id IS NULL OR c.status_id = 7)
  AND c.trashed_date IS NULL
  AND c.lead_status IS NOT NULL
  AND (ca.address_id IS NULL OR ca.address_id IN ( 
         SELECT cta.address_id 
         FROM contact_address cta 
         WHERE cta.contact_id = c.contact_id AND cta.primary_address = $P{TRUE})
       OR ca.address_id IN (
         SELECT MIN(ctadd.address_id) FROM contact_address ctadd WHERE ctadd.contact_id = c.contact_id AND
         ctadd.contact_id NOT IN (SELECT contact_id FROM contact_address WHERE contact_address.primary_address = $P{TRUE})))
  ORDER BY c.namelast, c.namefirst]]></queryString>
