	<queryString><![CDATA[
    SELECT org.name as account, dep.description as department, 
    rl."role", acc.username, 
    con.namelast as lastname, con.namefirst as firstname, 
    log.ip, log.entered
    FROM access_log log
    LEFT JOIN contact con ON (log.user_id = con.user_id)
    LEFT JOIN lookup_department dep ON (dep.code = con.department)
    LEFT JOIN "access" acc ON (acc.user_id = log.user_id)
    LEFT JOIN "role" rl ON (rl.role_id = acc.role_id)
    LEFT JOIN organization org ON (org.org_id = con.org_id)
    WHERE org.enabled = $P{TRUE}
    AND log.entered >= $P{date_start}
    AND log.entered <= $P{date_end} 
    $P!{orgid_where}
    $P!{lookup_role_where}
    $P!{lookup_department_where}
    ORDER BY org.name, con.namelast, acc.username, dep.description, rl."role", log.entered
   ]]></queryString>
