	<queryString><![CDATA[SELECT t.entered,
t.problem,  
t.solution,
t.resolution_date,
co.namefirst,
co.namelast,
o.name
FROM ticket t
LEFT JOIN organization o ON (t.org_id = o.org_id)
LEFT JOIN contact co ON (t.assigned_to = co.user_id)
WHERE t.ticketid > 0
AND t.closed IS NOT NULL
$P!{orgid_where}
ORDER BY o.name, t.entered]]></queryString>
