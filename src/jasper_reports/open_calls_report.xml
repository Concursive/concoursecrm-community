	<queryString><![CDATA[SELECT t.entered,
t.problem,  
ts.description,
co.namefirst,
co.namelast,
o.name
FROM ticket t
LEFT JOIN organization o ON (t.org_id = o.org_id)
LEFT JOIN ticket_severity ts ON (t.scode = ts.code) 
LEFT JOIN contact co ON (t.assigned_to = co.user_id)
WHERE t.ticketid > 0 
AND t.closed IS NULL
ORDER BY t.entered, o.name]]></queryString>
