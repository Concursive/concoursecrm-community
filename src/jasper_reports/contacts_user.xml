	<queryString><![CDATA[SELECT 
con.namefirst,con.namelast,con.contact_id,con.title,
lct.description as contact_type,o.name, 
con.status_id
FROM contact con
LEFT JOIN contact_type_levels ctl ON (con.contact_id = ctl.contact_id)
LEFT JOIN lookup_contact_types lct ON (ctl.type_id = lct.code)
LEFT JOIN organization o ON (con.org_id = o.org_id)
WHERE con.owner = $P{userid}
AND (con.status_id IS NULL OR con.status_id = 7)
AND con.lead = $P{FALSE}
ORDER BY con.namelast, con.namefirst]]></queryString>
