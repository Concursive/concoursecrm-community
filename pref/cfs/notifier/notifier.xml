<!-- 
  Configuration for Notifier
  AUTO-GENERATED BY CFS INSTALLER
-->
<application>
  <!-- Driver for connecting to sites database -->
  <init-param>
    <param-name>GATEKEEPER.DRIVER</param-name>
    <param-value>@GATEKEEPER.DRIVER@</param-value>
  </init-param>
  <init-param>
      <param-name>GATEKEEPER.URL</param-name>
      <param-value>@GATEKEEPER.URL@</param-value>
  </init-param>
  <init-param>
      <param-name>GATEKEEPER.USER</param-name>
      <param-value>@GATEKEEPER.USER@</param-value>
  </init-param>
  <init-param>
      <param-name>GATEKEEPER.PASSWORD</param-name>
      <param-value>@GATEKEEPER.PASSWORD@</param-value>
  </init-param>
  <!-- Web address for CFS Web Server to retrieve messages from -->
  <init-param>
    <param-name>CFSWebServer</param-name>
    <param-value>127.0.0.1</param-value>
  </init-param>
  <init-param>
    <param-name>FileLibrary</param-name>
    <param-value>@FILELIBRARY@</param-value>
  </init-param>
  <init-param>
    <param-name>MailServer</param-name>
    <param-value>@MAILSERVER@</param-value>
  </init-param>
  <!-- Remote Hylafax fax server -->
  <init-param>
    <param-name>FaxServer</param-name>
    <param-value>@FAXSERVER@</param-value>
  </init-param>
	<init-param>
    <param-name>FaxEnabled</param-name>
    <param-value>true</param-value>
  </init-param>
  <!-- Temporary folder for fax images -->
  <init-param>
    <param-name>BaseDirectory</param-name>
    <param-value>faxFiles</param-value>
  </init-param>
  <init-param>
    <param-name>BaseFilename</param-name>
    <param-value>cfsfax</param-value>
  </init-param>
</application>
