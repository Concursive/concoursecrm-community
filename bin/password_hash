#!/bin/sh

#############################################################################
#
# Generates an ASPCFS password
#
#############################################################################

usage ()
{
	printf "\nUsage: ${PROGNAME} [password]\n\n"
	exit 1
}

PASSWORD=$1
if [ -z "${PASSWORD}" ] ; then echo "Password not specified"; usage; fi
HASH=`java -classpath production/webapps/cfs2/WEB-INF/lib/darkhorseventures.jar com.darkhorseventures.utils.PasswordHash ${PASSWORD}`
echo "${PASSWORD}: ${HASH}"

